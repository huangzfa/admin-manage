<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.duobei.core.auth.dao.OperatorLoginLogDao" >

    <resultMap id="BaseResultMap" type="com.duobei.core.auth.domain.OperatorLoginLog" extends="com.duobei.core.auth.dao.mapper.OperatorLoginLogMapper.BaseResultMap">
    </resultMap>

    <select id="getByOpId" parameterType="java.lang.Integer" resultType="com.duobei.core.auth.domain.OperatorLoginLog">
        select * from aa_operator_login_log where op_id = #{opId} and login_type=1
        order by id desc limit 1
    </select>
</mapper>